{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    }
  },
  "sequential-thinking": {
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-sequential-thinking"
    ]
  },
  "agent-architect": {
    "command": "npx",
    "args": [
      "-y",
      "@agent-architect/mcp",
      "start"
    ],
    "env": {
      "AGENT_STORE_DIR": "./agents",
      "DEFAULT_MODEL": "claude-3.7-sonnet",
      "FAIL_FAST": "false"
    }
  }
}